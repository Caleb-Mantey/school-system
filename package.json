{
  "name": "school_system",
  "version": "0.0.1",
  "description": "A school management system",
  "author": "",
  "private": true,
  "license": "PROPRIETARY (copyright 2022)",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/src/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest --runInBand --force-exit",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage --runInBand --force-exit",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "typeorm": "node --require ts-node/register ./node_modules/typeorm/cli.js -d ./datasource.ts",
    "db:generate": "grunt exec:create_migration:",
    "db:migrate": "npm run build && npm run typeorm migration:run",
    "migration": "npm run typeorm migration:run",
    "db:rollback": "npm run typeorm migration:revert ",
    "migration:create": "npm run typeorm migration:create",
    "g:controller": "grunt exec:controller:",
    "g:service": "grunt exec:service:",
    "g:module": "grunt exec:module:",
    "g:gateway": "grunt exec:gateway:",
    "g:guard": "grunt exec:guard:",
    "g:decorator": "grunt exec:decorator:",
    "g:interceptor": "grunt exec:interceptor:",
    "g:interface": "grunt exec:interface:",
    "compodoc": "npx compodoc -p tsconfig.json -n \"Documentation\" ",
    "deploy:staging": "grunt deploy:staging",
    "setup:staging": "grunt setup:staging",
    "logs:staging": "grunt logs:staging",
    "logs:prod": "grunt logs:prod",
    "rollback:staging": "grunt rollback:staging",
    "db:backup:staging": "grunt backupDB:staging",
    "db:download:staging": "grunt getDB:staging"
  },
  "dependencies": {
    "@adminjs/express": "^5.0.0",
    "@adminjs/import-export": "^2.0.2",
    "@adminjs/nestjs": "^5.0.0",
    "@adminjs/typeorm": "^4.0.0",
    "@nestjs/axios": "0.0.8",
    "@nestjs/bull": "^0.6.2",
    "@nestjs/common": "^8.0.0",
    "@nestjs/core": "^8.0.0",
    "@nestjs/event-emitter": "^1.4.1",
    "@nestjs/mapped-types": "^1.0.1",
    "@nestjs/passport": "^8.2.1",
    "@nestjs/platform-express": "^8.0.0",
    "@nestjs/platform-socket.io": "^8.4.6",
    "@nestjs/schedule": "^2.1.0",
    "@nestjs/serve-static": "^2.2.2",
    "@nestjs/swagger": "^5.2.0",
    "@nestjs/typeorm": "^8.0.3",
    "@nestjs/websockets": "^8.4.6",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/otp-generator": "^4.0.0",
    "@types/redis": "^4.0.11",
    "adminjs": "^6.0.6",
    "axios": "^0.27.2",
    "bcrypt": "^5.0.1",
    "bignumber.js": "^9.0.1",
    "bull": "^4.10.2",
    "cache-manager": "^3.6.1",
    "cache-manager-redis-store": "^2.0.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "dotenv": "^16.0.2",
    "empty-value": "^1.0.1",
    "express": "^4.18.1",
    "express-formidable": "^1.2.0",
    "express-session": "^1.17.3",
    "georedis": "^3.1.3",
    "ioredis": "^5.1.0",
    "jsonwebtoken": "^8.5.1",
    "moment": "^2.29.3",
    "mysql2": "^2.3.3",
    "nestjs-slack": "^2.0.0",
    "nestjs-telegraf": "^2.6.0",
    "otp-generator": "^4.0.0",
    "passport": "^0.5.2",
    "passport-jwt": "^4.0.0",
    "redis": "^4.0.6",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.2.0",
    "sha1": "^1.1.1",
    "swagger-ui-express": "^4.3.0",
    "telegraf": "^4.8.6",
    "typeorm": "^0.3.0",
    "uuid": "^9.0.0",
    "voca": "^1.4.0"
  },
  "devDependencies": {
    "@compodoc/compodoc": "^1.1.19",
    "@nestjs/cli": "^8.0.0",
    "@nestjs/schematics": "^8.0.0",
    "@nestjs/testing": "^8.0.0",
    "@types/bull": "^4.10.0",
    "@types/cache-manager": "^3.4.3",
    "@types/cron": "^2.0.0",
    "@types/express": "^4.17.13",
    "@types/jest": "27.0.2",
    "@types/node": "^16.11.60",
    "@types/socket.io": "^3.0.2",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "grunt": "^1.6.1",
    "grunt-exec": "^3.0.0",
    "jest": "^27.2.5",
    "prettier": "^2.3.2",
    "source-map-support": "^0.5.20",
    "supertest": "^6.1.3",
    "ts-jest": "^27.0.3",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.0.0",
    "tsconfig-paths": "^3.10.1",
    "typescript": "^4.3.5"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coveragePathIgnorePatterns": [
      "<rootDir>/db/",
      "<rootDir>/app/mocks/",
      "<rootDir>/app/modules/",
      "<rootDir>/app/decorators/"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
